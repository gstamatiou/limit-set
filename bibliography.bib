@article{pozzetti_anosov_2023,
	title = {Anosov representations with {Lipschitz} limit set},
	volume = {27},
	issn = {1364-0380, 1465-3060},
	url = {http://arxiv.org/abs/1910.06627},
	doi = {10.2140/gt.2023.27.3303},
	abstract = {We study Anosov representations whose limit set has intermediate regularity, namely is a Lipschitz submanifold of a flag manifold. We introduce an explicit linear functional, the unstable Jacobian, whose orbit growth rate is integral on this class of representations. We prove that many interesting higher rank representations, including \${\textbackslash}Theta\$-positive representations, belong to this class, and establish several applications to rigidity results on the orbit growth rate in the symmetric space.},
	number = {8},
	urldate = {2024-03-28},
	journal = {Geometry \& Topology},
	author = {Pozzetti, Beatrice and Sambarino, Andrés and Wienhard, Anna},
	month = nov,
	year = {2023},
	note = {arXiv:1910.06627 [math]},
	keywords = {Mathematics - Differential Geometry, Mathematics - Dynamical Systems, Mathematics - Group Theory},
	pages = {3303--3360},
	file = {arXiv Fulltext PDF:/home/user/Zotero/storage/SA4ZKCJD/Pozzetti et al. - 2023 - Anosov representations with Lipschitz limit set.pdf:application/pdf;arXiv.org Snapshot:/home/user/Zotero/storage/SCD6KD7H/1910.html:text/html;Full Text PDF:/home/user/Zotero/storage/WCVXN25I/Pozzetti et al. - 2023 - Anosov representations with Lipschitz limit set.pdf:application/pdf},
